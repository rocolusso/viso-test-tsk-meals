{
  "extends": ["airbnb", "airbnb/hooks"],

  "parser": "@typescript-eslint/parser",
  "plugins": ["@typescript-eslint"],

  "settings": {
    "import/resolver": {
      "typescript": {

      }
    }
  },
  "rules": {
    "react/jsx-filename-extension": ["error", { "extensions": [".jsx", ".tsx"] }],
    "import/extensions": "off",
    "react/jsx-props-no-spreading": "off"
  }
}
